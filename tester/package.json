{
  "type": "module",
  "author": "ecoricemon",
  "name": "tester",
  "version": "0.0.0",
  "scripts": {
    "wasm-common": "cross-env RUSTFLAGS=\"-C target-feature=+atomics,+bulk-memory,+mutable-globals\" rustup run nightly-2024-06-20 wasm-pack build --target web --out-name wasm-index --out-dir pkg .",
    "wasm-d": "npm run wasm-common -- --dev -Z build-std=panic_abort,std",
    "wasm-r": "npm run wasm-common -- --release -Z build-std=panic_abort,std",
    "bundle-d": "vite build --mode development",
    "bundle-r": "vite build",
    "clean": "rimraf --glob dist pkg test-results",
    "clean-all": "rimraf --glob dist pkg test-results target node_modules",
    "build-d": "run-s clean wasm-d bundle-d",
    "build-r": "run-s clean wasm-r bundle-r",
    "start": "vite preview",
    "test": "playwright test"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "npm-run-all": "4.1.5",
    "rimraf": "6.0.1",
    "rollup-plugin-copy": "3.5.0",
    "vite": "5.4.2",
    "vite-plugin-cross-origin-isolation": "0.1.6",
    "vite-plugin-top-level-await": "1.4.4",
    "vite-plugin-wasm": "3.3.0",
    "playwright": "1.46.1",
    "@playwright/test": "1.46.1"
  }
}
